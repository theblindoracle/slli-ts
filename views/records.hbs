<html lang='en'>
  <head>
    <meta charset='UTF-8' />
    <meta name='viewport' content='width=device-width, initial-scale=1.0' />
    <title>Handlebars Table Template</title>
    <style>
      table { border-collapse: collapse; width: 100%; } th, td { border: 1px
      solid black; padding: 8px; text-align: left; } th { background-color:
      #f2f2f2; }
    </style>
  </head>
  <body>
    <nav>
      <ul>
        <li><a href='/slli/session'>Sessions</a></li>
        <li><a href='/slli/records'>Records</a></li>
        <li><a href='/slli/'>Record Aggregation</a></li>
      </ul>
    </nav>
    <h1>Records</h1>
    <table>
      <thead>
        <tr>
          <th>Weight</th>
          <th>Record Level</th>
          <th>Weight Class ID</th>
          <th>Discipline</th>
          <th>Division</th>
          <th>Equipment Level</th>
          <th>Sex</th>
          <th>US State</th>
        </tr>
      </thead>
      <tbody>
        {{#each records}}
          <tr>
            <td>{{weight}}</td>
            <td>{{recordLevel}}</td>
            <td>{{weightClassID}}</td>
            <td>{{discipline}}</td>
            <td>{{division}}</td>
            <td>{{equipmentLevel}}</td>
            <td>{{sex}}</td>
            <td>{{usState}}</td>
          </tr>
        {{/each}}
      </tbody>
    </table>

  </body>
</html>
